[sub_resource type="Shader" id="Shader_blur"]
code = "shader_type canvas_item;

uniform float blur_amount : hint_range(0.0, 5.0) = 2.0;
uniform float darken : hint_range(0.0, 1.0) = 0.3;

void fragment() {
    vec2 ps = TEXTURE_PIXEL_SIZE;
    vec4 color = vec4(0.0);

    float weights[9] = {0.05, 0.09, 0.12, 0.15, 0.18, 0.15, 0.12, 0.09, 0.05};

    for (int i = -4; i <= 4; i++) {
        for (int j = -4; j <= 4; j++) {
            vec2 offset = vec2(float(i), float(j)) * ps * blur_amount;
            color += texture(TEXTURE, UV + offset) * weights[i + 4] * weights[j + 4];
        }
    }

    color.rgb *= (1.0 - darken);
    COLOR = color;
}"

